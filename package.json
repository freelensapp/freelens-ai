{
  "name": "@freelensapp/freelens-ai",
  "version": "0.0.1",
  "description": "AI capabilities for Freelens",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freelensapp/freelens-ai.git",
    "directory": "."
  },
  "main": "dist/main.js",
  "renderer": "dist/renderer.js",
  "engines": {
    "node": ">= 22.14.0",
    "freelens": "^1.2.1"
  },
  "copyright": "Â© 2025 Freelens Authors",
  "license": "MIT",
  "author": {
    "name": "Freelens Authors",
    "email": "freelens@freelens.app"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm dlx cross-env@7.0.3 NODE_ENV=production webpack --progress",
    "pack": "pnpm pack",
    "biome": "pnpm dlx @biomejs/biome@1.9.4",
    "biome:check": "pnpm biome check",
    "biome:fix": "pnpm biome check --fix",
    "bump-version": "pnpm version --no-commit-hooks --no-git-tag-version",
    "clean": "pnpm dlx rimraf@6.0.1 binaries dist static/build",
    "clean:node_modules": "pnpm dlx rimraf@6.0.1 node_modules",
    "knip": "pnpm dlx knip@5.55.1 --dependencies",
    "knip:development": "pnpm knip",
    "knip:production": "pnpm knip --production --strict",
    "lint": "pnpm biome:check",
    "lint:fix": "pnpm biome:fix"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@freelensapp/extensions": "^1.3.0",
    "@langchain/core": "^0.3.55",
    "@langchain/google-genai": "^0.2.10",
    "@langchain/langgraph": "^0.2.72",
    "@langchain/mcp-adapters": "^0.5.2",
    "@langchain/ollama": "^0.2.0",
    "@langchain/openai": "^0.5.10",
    "@types/react-syntax-highlighter": "^15.5.13",
    "langchain": "^0.3.25",
    "lucide-react": "^0.510.0",
    "mobx": "^6.13.7",
    "mobx-react": "^9.2.0",
    "react": "^19.1.0",
    "react-markdown": "^6.0.3",
    "react-syntax-highlighter": "^15.6.1",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/node": "^22.15.17",
    "@types/react": "19.1.4",
    "@types/react-dom": "^19.1.2",
    "css-loader": "^7.1.2",
    "raw-loader": "^4.0.2",
    "react-dom": "^19.1.0",
    "sass": "^1.87.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "5.8.3",
    "vite": "^6.3.5",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1"
  },
  "keywords": [
    "extension",
    "freelensapp",
    "lens",
    "openlens",
    "freelens"
  ],
  "packageManager": "pnpm@10.12.1"
}
